# Copy this to .env.local and fill in your values

# Dashboard Authentication
DASHBOARD_PASSWORD_HASH=
JWT_SECRET=

# AWS Configuration
# Note: AWS Amplify blocks AWS_* prefixed variables, so use CUSTOM_AWS_* prefix
# For AWS Amplify: Configure IAM service role instead (recommended)
# For local/Docker: Use CUSTOM_AWS_* variables below
CUSTOM_AWS_REGION=
CUSTOM_AWS_ACCESS_KEY_ID=
CUSTOM_AWS_SECRET_ACCESS_KEY=

# DynamoDB Tables
DYNAMODB_BRANDS_TABLE=
DYNAMODB_EVENTS_TABLE=
DYNAMODB_LEGACY_TABLE=

# API Gateway
NEXT_PUBLIC_API_GATEWAY_URL=

# App Configuration
NEXT_PUBLIC_APP_NAME=
NEXT_PUBLIC_APP_URL=
